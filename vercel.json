{
  "version": 2,
  "builds": [
    {
      "src": "public/index.html",
      "use": "@vercel/static"
    },
    {
      "src": "public/*.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/public/index.html"
    },
    {
      "src": "/.well-known/farcaster.json",
      "dest": "/public/.well-known/farcaster.json"
    }
  ]
}
