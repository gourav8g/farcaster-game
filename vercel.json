{
  "version": 2,
  "builds": [
    {
      "src": "index.html",
      "use": "@vercel/static"
    },
    {
      "src": "*.json",
      "use": "@vercel/static"
    }
  ],
  "routes": [
    {
      "src": "/(.*)",
      "dest": "/index.html"
    },
    {
      "src": "/.well-known/farcaster.json",
      "dest": "/.well-known/farcaster.json",
      "headers": {
        "Content-Type": "application/json"
      }
    }
  ]
}
