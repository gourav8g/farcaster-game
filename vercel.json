{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/.well-known/farcaster.json",
      "headers": { "Content-Type": "application/json" },
      "continue": true
    },
    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}
